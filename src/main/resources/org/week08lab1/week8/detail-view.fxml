<AnchorPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="org.week08lab1.week8.DetailController"
            prefHeight="480.0" prefWidth="640.0">
    <VBox fx:id="centerVBox" alignment="CENTER" spacing="0"
          AnchorPane.topAnchor="0.0" AnchorPane.bottomAnchor="0.0"
          AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0">
        <HBox><Button text="Back" onAction="#handleBackButtonClick" /></HBox>
        <HBox alignment="CENTER" spacing="10.0">
            <Label>The Model is: </Label>
            <Label fx:id="planeModel"></Label>
        </HBox>
        <HBox alignment="CENTER" spacing="20.0">
            <ImageView fx:id="detailImageView" fitHeight="360" fitWidth="360">

            </ImageView>
        </HBox>
    </VBox>


</AnchorPane>
