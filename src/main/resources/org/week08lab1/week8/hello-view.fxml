<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.image.Image?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.HBox?>
<VBox alignment="CENTER" spacing="20.0" xmlns:fx="http://javafx.com/fxml"
      fx:controller="org.week08lab1.week8.HelloController">

    <HBox alignment="CENTER" spacing="20.0">
        <VBox alignment="CENTER" spacing="5.0">
            <Button mnemonicParsing="false" onAction="#detailview" text="" fx:id="A333">
                <graphic>
                    <ImageView fitHeight="150.0" fitWidth="200.0" pickOnBounds="true" preserveRatio="true">
                        <Image url="@A333.png"/>
                    </ImageView>
                </graphic>
            </Button>
            <Label>Airbus A330-300</Label>
        </VBox>
        <VBox alignment="CENTER" spacing="5.0">
            <Button mnemonicParsing="false" onAction="#detailview" text="" fx:id="B77L">
                <graphic>
                    <ImageView fitHeight="150.0" fitWidth="200.0" pickOnBounds="true" preserveRatio="true">
                        <Image url="@B77L.png"/>
                    </ImageView>
                </graphic>
            </Button>
            <Label>Boeing777-300</Label>
        </VBox>
        <VBox alignment="CENTER" spacing="5.0">
            <Button mnemonicParsing="false" onAction="#detailview" text="" fx:id="B77W">
                <graphic>
                    <ImageView fitHeight="150.0" fitWidth="200.0" pickOnBounds="true" preserveRatio="true">
                        <Image url="@B77W.png"/>
                    </ImageView>
                </graphic>
            </Button>
            <Label>Boeing 777-200</Label>
        </VBox>
    </HBox>
    <HBox alignment="CENTER" spacing="20.0">
        <VBox alignment="CENTER" spacing="5.0">
            <Button mnemonicParsing="false" onAction="#detailview" text="" fx:id="B788">
                <graphic>
                    <ImageView fitHeight="150.0" fitWidth="200.0" pickOnBounds="true" preserveRatio="true">
                        <Image url="@B788.png"/>
                    </ImageView>
                </graphic>
            </Button>
            <Label>Boeing 787-200</Label>
        </VBox>
        <VBox alignment="CENTER" spacing="5.0">
            <Button mnemonicParsing="false" onAction="#detailview" text="" fx:id="B789">
                <graphic>
                    <ImageView fitHeight="150.0" fitWidth="200.0" pickOnBounds="true" preserveRatio="true">
                        <Image url="@B789.png"/>
                    </ImageView>
                </graphic>
            </Button>
            <Label>Boeing 777-200</Label>
        </VBox>
    </HBox>
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0"/>
    </padding>

    <Label fx:id="welcomeText"/>
    <Button text="Hello!" onAction="#onHelloButtonClick"/>
</VBox>
